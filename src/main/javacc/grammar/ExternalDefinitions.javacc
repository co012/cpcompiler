TranslationUnit :
    (ExternalDeclaration)+
;


ExternalDeclaration:
    SCAN 3 ClassDefinition
    | SCAN Declaration
    | FunctionDefinition
;

FunctionDefinition : DeclarationSpecifiers Declarator [DeclarationList] CompoundStatement ;

ClassDefinition : <CLASS> <IDENTIFIER> "{" [StructDeclarationList] (FunctionDefinition)* "}" ;

DeclarationList :
    (Declaration)+
;

